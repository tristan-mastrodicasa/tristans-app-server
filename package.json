{
  "name": "tristans-app-server",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "apidoc": "apidoc -o api-doc -i src/routers",
    "build": "tsc --project tsconfig.prod.json",
    "startDev": "NODE_PATH=./dist NODE_ENV=development node ./dist/server.js",
    "start": "NODE_PATH=./dist node ./dist/server.js",
    "dev": "NODE_PATH=./src NODE_ENV=development nodemon",
    "typeorm": "NODE_PATH=./src ts-node ./node_modules/typeorm/cli.js",
    "lint": "tslint src/**/*.ts",
    "test": "NODE_PATH=./src NODE_ENV=development ts-node node_modules/jasmine/bin/jasmine",
    "testPushNotifications": "NODE_PATH=./src NODE_ENV=development ts-node src/spec-helpers/test-push-notifications.ts",
    "populateDatabase": "NODE_PATH=./src NODE_ENV=development ts-node src/spec-helpers/populate-database.ts",
    "scss": "node-sass --watch src/scss -o assets/css",
    "resetContentTime": "NODE_PATH=./src ts-node src/spec-helpers/reset-content-time.ts"
  },
  "apidoc": {
    "title": "DopaMeme",
    "url": "https://localhost:3000/api"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/express-rate-limit": "^3.3.3",
    "@types/faker": "^4.1.5",
    "@types/form-urlencoded": "^2.0.1",
    "@types/jasmine": "^3.3.13",
    "@types/jsonwebtoken": "^8.3.3",
    "@types/multer": "^1.3.9",
    "@types/multer-s3": "^2.7.7",
    "@types/mysql2": "types/mysql2",
    "@types/node": "^12.0.12",
    "@types/passport": "^1.0.0",
    "@types/passport-facebook-token": "^0.4.33",
    "@types/passport-google-oauth20": "^2.0.2",
    "@types/passport-jwt": "^3.0.1",
    "@types/supertest": "^2.0.8",
    "faker": "^4.1.0",
    "form-urlencoded": "^4.0.1",
    "jasmine": "^3.4.0",
    "js-image-generator": "^1.0.2",
    "node-sass": "^4.13.0",
    "nodemon": "^1.19.0",
    "supertest": "^4.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "aws-sdk": "^2.554.0",
    "axios": "^0.19.0",
    "class-validator": "^0.9.1",
    "ejs": "^2.7.1",
    "express": "^4.16.4",
    "express-rate-limit": "^5.0.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "mysql2": "^1.6.5",
    "passport": "^0.4.0",
    "passport-facebook-token": "^3.3.0",
    "passport-google-authcode": "^0.1.5",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "typeorm": "^0.2.25"
  }
}
