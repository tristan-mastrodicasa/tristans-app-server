{
  "extends": "tslint-config-airbnb",
  "rules": {
    "completed-docs": [
      true,
      {
        "enums": true,
        "methods": true
      }
    ],
    "max-line-length": [
      false,
      100
    ],
    "variable-name": {
      "options": [
        "allow-leading-underscore"
      ]
    }
  }
}
